import stdio

fun fibonacci(start i32, limit i32) -> i32:
    var prev i32 = start, current i32 = start, temp i32 = 0

    while current < limit:
        temp = current
        current = current + prev
        prev = temp
    
    return current

fun main() -> i32:
    printf("%d", fibonacci(1, 100))
    putchar(10)
