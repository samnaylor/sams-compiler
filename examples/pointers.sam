import stdio


type ChangeMe:
    value i32


fun change(cm ptr ChangeMe, new_value i32) -> i32:
    (deref cm).value = new_value
    return 0


fun plus_one (x ptr i32) -> i32:
    (deref x) = (deref x) + 1
    return 0

fun main() -> i32:
    var x i32 = 5
    plus_one(ref x)

    var cm ChangeMe = ChangeMe(5)
    change(ref cm, 10)

    printf("%d", cm.value)
    putchar(10)

    return x
