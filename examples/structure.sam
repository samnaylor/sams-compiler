import stdio


extern sqrt(x i32) -> i32


type Point:
    x i32
    y i32


fun dist(p Point) -> i32:
    return (sqrt(((p.x * p.x) + (p.y * p.y))))


fun main() -> i32:
    var p Point = Point(3, 4)
    printf("Point:   (%d, %d)", p.x, p.y)
    putchar(10)
    printf("Distance: %d", dist(p))
    putchar(10)
    return 0
